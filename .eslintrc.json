{
  "root": true,
  "extends": [
    "@raycast"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error", 
      { 
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-unused-vars": "off",
    "no-confusing-arrow": ["error", { "allowParens": true }]
  }
} 